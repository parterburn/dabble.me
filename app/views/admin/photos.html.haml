- title ("Admin Photos")

:css
  .col-md-3 img {
    width: 100%;
  }

.row.s-admin-container
  .col-md-12
    %h3
      Admin Photos

  .col-md-12
    %hr

  .col-md-12
    .row
      .center
        = paginate @entries
    .row
      - @entries.each do |entry|
        .pictureFrame.col-xs-2.col-md-3
          =link_to entry.image_url_cdn, target: "_blank", style: "border: 1px solid black; display: inline-block;" do
            =image_code(entry)
    .row
      .center
        = paginate @entries
