- title "#{@title} on Dabble Me"
.row
  = render "sidebar"
  = render "search"

  .col-md-8.col-md-offset-1
    .center
      = paginate @entries

  - @entries.each do |entry|
    .col-md-8.col-md-offset-1.well.entry{:id => "entry-#{entry.id}"}
      = render partial: 'entries/entry', locals: { entry: entry }

  .col-md-8.col-md-offset-1
    .center
      = paginate @entries
